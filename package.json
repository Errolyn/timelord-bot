{
  "name": "time-lord-bot",
  "version": "0.0.1",
  "description": "A discord bot",
  "main": "index.js",
  "author": "Errolyn",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config index.js",
    "lint": "eslint . --ext .js",
    "format": "prettier ./.eslintrc.json -c **/*"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eris": "^0.13.1"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
